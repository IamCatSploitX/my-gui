spamscript.Name             = "spam script"
spamscript.Parent           = Main
spamscript.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
spamscript.Position         = UDim2.new(0.0514138825, 0, 0.511415482, 0)
spamscript.Size             = UDim2.new(0, 160, 0, 35)
spamscript.Font             = Enum.Font.SourceSans
spamscript.Text             = "Tp Walk"
spamscript.TextColor3       = Color3.fromRGB(0, 255, 149)
spamscript.TextSize         = 14.000
spamscript.MouseButton1Down:connect(function()
local Keydown = "g"

local m = game:GetService"Players".LocalPlayer:GetMouse()
m.KeyDown:connect(function(k)
   if k == Keydown then
game.ReplicatedStorage.Events.WandEvent:FireServer("apparate", false)
   end
   end)
    local Key = "g" -- Key that toggles script
    local plr = game.Players.LocalPlayer
    local char       =  plr.Character
    local mouse       =  plr:GetMouse()
    local KeyDown = false
    Toggle  = false

mouse.KeyDown:connect(function(key)
        if key == Key then
            if Toggle == true then
                Toggle = false
            else if Toggle == false then
                    Toggle = true
                end
            end
        end
    end)

spawn(function() -- this starts the function if its on
        while wait() do -- this is a loop if its waiting then it will start the stuff below
            if Toggle == true then -- this is the requirement to turn it on
                game.ReplicatedStorage.Events.WandEvent:FireServer("Cloak")
                wait(0.1)
                game.ReplicatedStorage.Events.WandEvent:FireServer("apparate")
                local plr     = game.Players.LocalPlayer    local char = plr.Character  char.Humanoid.WalkSpeed =199
                elseif Toggle == false then
        local plr = game.Players.LocalPlayer    local char = plr.Character  char.Humanoid.WalkSpeed =16
            end
            end
            end)
            end)

autograbgalleons.Name = "auto grab galleons"
autograbgalleons.Parent = Main
autograbgalleons.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
autograbgalleons.Position = UDim2.new(0.526992261, 0, 0.511415482, 0)
autograbgalleons.Size = UDim2.new(0, 160, 0, 35)
autograbgalleons.Font = Enum.Font.SourceSans
autograbgalleons.Text = "Auto Grab Galleons"
autograbgalleons.TextColor3 = Color3.fromRGB(0, 255, 149)
autograbgalleons.TextSize = 14.000
autograbgalleons.MouseButton1Down:connect(function()
    local TweenService = game:GetService("TweenService")

local LocalPlayer = game.Players.LocalPlayer

function TweenPlayerToPart(Position)
    local cframe = CFrame.new(Position)

    local info = TweenInfo.new(1)
    
    local tween = TweenService:Create(LocalPlayer.Character.HumanoidRootPart, info, {CFrame = cframe})

    tween:Play()
end

if workspace:FindFirstChild("Loots") then
    --workspace.Loots.Loot.Loot.Size = Vector3.new(1000, 1000, 1000)
    TweenPlayerToPart(workspace.Loots.Loot.Loot.Position)
end

workspace.DescendantAdded:Connect(function(descendant)
    if descendant.Name == "Loot" then
        if descendant:IsA("Model") then
            local LootBag = descendant:FindFirstChild("Loot")

            if LootBag:IsA("BasePart") or LootBag:IsA("MeshPart") then
                --LootBag.Size = Vector3.new(1000, 1000, 1000)
                TweenPlayerToPart(LootBag.Position)
            end
        elseif descendant:IsA("MeshPart") then
            --descendant.Size = Vector3.new(1000, 1000, 1000)
            TweenPlayerToPart(descendant.Position)
        end
    end
end)
end)

