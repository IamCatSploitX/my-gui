local q   = "crucio"
    local Key = "e" -- Key that toggles script
    local plr       = game.Players.LocalPlayer
    local char       =  plr.Character
    local mouse       =  plr:GetMouse()
    local crucio  = false

    function cruciospam()
        local v = (mouse.Hit.p - char.HumanoidRootPart.Position).unit/2
        local w = v *  (300000000)
        local x = math.random(1, 100)
        local y = char.HumanoidRootPart.Position + -v * -1
        plr.PlayerGui.Client.fwp:Fire(y, w, true, q)
        game.ReplicatedStorage.Events.ProjEvent:FireServer(y, w, q, x)
    end

    game:GetService("Players").LocalPlayer.Chatted:Connect(function(msg)
   if string.lower(msg) == ".spamcrucio" and crucio == false then
      crucio = true
   elseif string.lower(msg) == ".spamcruciooff" and crucio == true then
   crucio = false
   end
end)



    mouse.Button1Down:connect(function()
        while crucio do
            cruciospam()
            wait(.001)
        end
    end)
    
