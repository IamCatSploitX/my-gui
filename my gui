Check.Name = "Check"
Check.Parent = KeyFrame
Check.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Check.Position = UDim2.new(0.277161866, 0, 0.728260875, 0)
Check.Size = UDim2.new(0, 200, 0, 50)
Check.Font = Enum.Font.SourceSans
Check.Text = "CHECK KEY"
Check.TextColor3 = Color3.fromRGB(1, 255, 107)
Check.TextSize = 46.000
Check.MouseButton1Down:connect(function()
    local function LZSRSGP_fake_script() -- Check.LocalScript 
        local script = Instance.new('LocalScript', Check)

        local key = script.Parent.Parent.key

        script.Parent.MouseButton1Click:Connect(function()
            if key.Text == "SploitX" then -- Make the "Key" whatever you wish.
                script.Parent.Parent:TweenPosition(UDim2.new(0.383,0,-0.9,0), "Out", "Quad", 1, true)
                script.Parent.Parent.Parent.Frame.Visible = true
                wait(5)
                script.Parent.Parent:Destroy() -- Destroys the GUI after a set time
                print("Destroyed!")
            elseif -- Tweening | If you want it to do a different tween just change the "Quad" to another tween animation
                key.Text == "" then
                key.Text = "" else
                key.Text = "You are not whitelisted dm CatSploitAdmin#9254."
                wait(1)
                key.Text = ""
            end
        end)

    end
    coroutine.wrap(LZSRSGP_fake_script)()
end)

